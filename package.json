{
  "name": "oceanic_odyssey",
  "version": "1.0.0",
  "description": "OceanicOdyssey [NodeJS-18.x Serverless Application Model] A piece from Twin Roses Back-End",
  "main": "./bin/functions/index/index.mjs",
  "type": "module",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/redvelvetme/oceanicodyssey.git"
  },
  "bugs": {
    "url": "https://github.com/redvelvetme/oceanicodyssey/issues",
    "email": "re@redvelvet.me"
  },
  "keywords": [
    "nodejs",
    "serverless",
    "backend",
    "api"
  ],
  "author": "<re@redvelvet.me> (https://redvelvet.me)",
  "contributors": [
    "<re@redvelvet.me> (https://redvelvet.me)"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 18.17.1",
    "npm": ">= 9.8.1",
    "yarn": ">= 1.22.19"
  },
  "scripts": {
    "local:set": "cd bin/helpers && npm i",
    "lint": "eslint \"**/*.{js,mjs,cjs}\"",
    "build": "npm run local:set && sam build",
    "build:docker": "npm run local:set && sam build --use-container",
    "deploy": "npm run lint && sam deploy --guided || echo \"Test fail or no changes to deploy, not deploying!\"",
    "deploy:force": "sam deploy",
    "delete": "sam delete --stack-name OceanicOdyssey"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "prettier": "^3.0.1"
  }
}